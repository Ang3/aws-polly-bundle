{
  "name": "ang3/aws-polly-bundle",
  "type": "symfony-bundle",
  "homepage": "https://github.com/Ang3/aws-polly-bundle",
  "description": "AWS Polly bundle for Symfony apps",
  "license": "proprietary",
  "authors": [
    {
      "name": "Joanis ROUANET",
      "homepage": "https://github.com/Ang3"
    }
  ],
  "require": {
    "php": ">=8.1",
    "async-aws/async-aws-bundle": "^1.10",
    "aws/aws-sdk-php": "^3.129",
    "symfony/config": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/http-kernel": "^6.0"
  },
  "require-dev": {
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {"Ang3\\Bundle\\AwsPollyBundle\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "Ang3\\Bundle\\AwsPollyBundle\\Tests\\": "tests/" }
  },
  "config": {
    "sort-packages": true
  }
}
